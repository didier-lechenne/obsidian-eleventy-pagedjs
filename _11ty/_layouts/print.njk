<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{% if title %}{{ title }} | {% endif %}{{ config.title }}</title>
    <meta name="description" content="{{ subtitle or config.subtitle }}" />

    <link rel="icon" type="image/svg+xml" href="./assets/favicon.svg" />

    <meta property="og:title" content="{{ title or config.title }}" />
    <meta
      property="og:description"
      content="{{ subtitle or config.subtitle }}"
    />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{ config.url }}{{ page.url }}" />

   
    
    <script src="./csspageweaver/lib/csstree.min.js"></script>
    <script src="./csspageweaver/main.js" type="module"></script>
  </head>

  <body class="print {{ bodyClass }}" lang="fr">
    <main class="main-content">
      <div class="container">
        {% for p in collections.printPages %} {% set templateName =
        p.data.template | default('default') %} {% include "templates/" +
        templateName + ".njk" %} {% endfor %}
      </div>
    </main>
  </body>
</html>