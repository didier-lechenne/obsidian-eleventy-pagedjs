{
  "name": "eleventy-pagedjs",
  "version": "1.0.0",
  "description": "Configuration Eleventy pour générer web et print avec Paged.js",
  "main": ".eleventy.js",
  "scripts": {
    "clean": "rimraf .cache _site",
    "build:web": "npm run clean && cross-env BUILD_ENV=web npx @11ty/eleventy",
    "build:print": "npm run clean && cross-env BUILD_ENV=print npx @11ty/eleventy",
    "build": "npm run build:print",
    "serve:web": "npm run clean && cross-env BUILD_ENV=web npx @11ty/eleventy --serve",
    "serve:print": "npm run clean && cross-env BUILD_ENV=print npx @11ty/eleventy --serve ",
    "serve": "npm run clean && cross-env npx @11ty/eleventy --serve ",
    "deploy": "sh deploy.sh",
    "stop": "kill-port 3000"
  },
  "keywords": ["eleventy", "print", "web", "paged-js", "static-site"],
  "author": "didier@lechenne.fr",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "clean-css": "^5.3.3",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "lightningcss": "^1.30.1",
    "lightningcss-cli": "^1.30.1",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.2",
    "@11tyrocks/eleventy-plugin-collection-schemas": "^0.1.1",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@uncenter/eleventy-plugin-toc": "^1.0.3",
    "adm-zip": "^0.5.16",
    "cross-env": "^10.0.0",
    "css-what": "^6.1.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.9.1",
    "kill-port": "^2.0.1",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-image-figures": "^2.1.1",
    "markdown-it-implicit-figures": "^0.12.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "rimraf": "^6.0.1"
  }
}
